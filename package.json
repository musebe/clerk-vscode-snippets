{
  "name": "clerkdocs-snippets",
  "displayName": "ClerkDocs Snippets",
  "description": "This extension provides code snippets for Clerk from its official documentation.",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "icon": "icon.png",
  "categories": [
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/musebe/clerk-vscode-snippets"
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "keywords": [
    "clerk",
    "snippets",
    "documentation",
    "auth",
    "authentication",
    "vscode",
    "extension",
    "code snippets",
    "clerk integration"
  ],
  "license": "MIT",
  "author": "Musebecodes",
  "devDependencies": {
    "@vscode/vsce": "^2.20.1",
    "jest": "^29.6.2"
  },
  "scripts": {
    "deploy": "vsce publish",
    "test": "jest",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "publisher": "Musebe",
  "dependencies": {
    "jsonc-parser": "^3.2.0"
  }
}
